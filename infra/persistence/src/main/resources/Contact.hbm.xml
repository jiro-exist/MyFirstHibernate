<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>
   <class name="com.exist.manio.myfirsthibernate.core.model.Contact" table="contact">
      <meta attribute="class-description">
         This class contains the Person's Contact details.
      </meta>

      <id name="contactId" type="int" column="contact_id">
         <generator class="identity"/>
      </id>

      <property name="id" column="id" type="int"/>
      <property name="contactCode" column="contact_code" type="string"/>
      <property name="contactValue" column="contact_value" type="string"/>
      
      <!-- <many-to-one name="person" class="com.exist.manio.myfirsthibernate.core.model.Person" fetch="select" lazy="false">
         <column name="id" not-null="true"/>
      </many-to-one> -->

   </class>
</hibernate-mapping>