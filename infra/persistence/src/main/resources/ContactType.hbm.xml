<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>
   <class name="com.exist.manio.myfirsthibernate.core.model.ContactType" table="contact_type">
      <meta attribute="class-description">
         This class contains the types of contacts.
      </meta>
      <property name="contactCode" column="contact_code" type="string"/>
      <property name="contactDesc" column="contact_desc" type="string"/>
   </class>
</hibernate-mapping>